<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PictoApp.Views.Usuarios.InicioCat">
    <ContentPage.Content>
        <AbsoluteLayout>
       
                <Image Source="LogoXamarin.png" HeightRequest="140" WidthRequest="140"  BackgroundColor="Transparent"/>
            <Entry x:Name="TxtEmail" Placeholder="Email" PlaceholderColor="DarkGray" TextColor="Black"/>
            <Entry x:Name="TxtContra" Placeholder="Contrasena" PlaceholderColor="DarkGray" TextColor="Black" IsPassword="True"/>
            <Button x:Name="BtnIngresar" Text="Ingresar" Clicked="BtnIngresar_Clicked"/>
            <Button x:Name="BtnRegistrarse" Text="¿No tenes cuenta? Resgistrate!" Clicked="BtnRegistrarse_Clicked"/>

        </AbsoluteLayout>
    </ContentPage.Content>
</ContentPage>